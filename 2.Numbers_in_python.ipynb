    "# Write a program to solve quadratic equation?\n",
    "import cmath\n",
    "a= 1\n",
    "b= 4 \n",
    "c= 2\n",
    "# Calculating the discrimnent\n",
    "dis = (b**2)-(4*a*c)\n",
    "# Find two results\n",
    "ans1 = (-b-cmath.sqrt(dis))/(2*a)\n",
    "ans2 = (-b+cmath.sqrt(dis))/(2*a)\n",
    "# The result are \n",
    "print(\"The root are :\")\n",
    "print(ans1)\n",
    "print(ans2)"
   ]
